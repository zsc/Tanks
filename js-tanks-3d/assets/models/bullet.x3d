<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE X3D PUBLIC "ISO//Web3D//DTD X3D 3.2//EN" "http://www.web3d.org/specifications/x3d-3.2.dtd">
<X3D profile='Interchange' version='3.2'>
  <head>
    <meta name='title' content='Bullet Model'/>
    <meta name='description' content='3D bullet/shell model for Tanks game'/>
    <meta name='creator' content='Claude'/>
  </head>
  <Scene>
    <!-- Bullet Shell -->
    <Transform DEF='BulletShell'>
      <Shape>
        <Appearance>
          <Material diffuseColor='0.7 0.6 0.2' specularColor='0.9 0.8 0.3' shininess='0.9' ambientIntensity='0.3'/>
        </Appearance>
        <Cylinder radius='0.05' height='0.2'/>
      </Shape>
    </Transform>
    
    <!-- Bullet Tip (cone) -->
    <Transform DEF='BulletTip' translation='0 0.15 0'>
      <Shape>
        <Appearance>
          <Material diffuseColor='0.8 0.7 0.3' specularColor='1.0 0.9 0.4' shininess='1.0' ambientIntensity='0.3'/>
        </Appearance>
        <Cone bottomRadius='0.05' height='0.1'/>
      </Shape>
    </Transform>
  </Scene>
</X3D>